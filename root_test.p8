function _draw()
	cls()
	map(0,0)
	draw_root()
end

function draw_root()
	spr(35,4*8,12*8)
	for c=1,root do
		if (c-root~=0) spr(19,4*8,(12-c)*8)
		if (c-root==0) spr(3,4*8,(12-c)*8)
	end
end

